<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Education\VisualFeedbackBundle\Entity\Image" table="Image">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="label" type="string" column="label" length="128"/>
    <field name="filename" type="string" column="filename" length="256"/>
    <many-to-one field="imagefolder" target-entity="Imagefolder" orphan-removal="">
      <join-columns>
        <join-column name="ImageFolder_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
