.dialog_delete_button
{
	color: #9E2E2E ! important;
	background: #FCDFDF ! important;
	border: 1px solid #ECC5C5 ! important;
}

.tooltip
{
	background-color:#5C9CCC;
	border:1px solid #fff;
	padding:5px;
	width:250px;
	display:none;
	color:#fff;
	text-align:left;
	font-size:12px;
	line-height: 12px;
	z-index: 1090;

	/* outline radius for mozilla/firefox only */
	-moz-box-shadow:0 0 10px #000;
	-webkit-box-shadow:0 0 10px #000;

	-moz-border-radius: 5px 5px 5px 5px;
}

.tooltip .data_point
{
  width: 200px;
  display: inline-block;
  margin: 0 ! important;
}

.tooltip .label
{
  width: 50px;
	color: #333333;
}

.tooltip .text
{
  line-height: 28px;
}

.mini_lesson_icon
{
	background-color: #ACDD4A;
	height: 120px;
	overflow-x: auto;
}

.mini_lesson_icon:hover
{
	background-color: #FCE877 ! important;
	cursor: hand; cursor: pointer;
}

.mini_lesson_icon .mini_icon
{
	-moz-box-shadow: 0px 0px 0px #000000 ! important;
}

.mini_lesson_icon .mini_icon:hover
{
	background-color: #FFFFFF ! important;
}

.mini_lesson_icon #lesson_name
{
	color: #000000;
	padding: 0px 0px 0px 5px;
}

.mini_icon
{
	background-color: #FFFFFF;
	margin: 0 0 10px 10px ! important;
}

.mini_icon:hover
{
	background-color: #FCE877 ! important;
	cursor: hand;
	cursor: pointer;
}

.mini_icon .image
{
	height: auto;
}

.mini_icon .image img
{
	height: 40px;
}

.dialog_icon
{
	margin: 35px 10px 10px 0 ! important;
}

#lesson_dialog .dialog_icon
{
	margin: 0px 0px 20px 0px !important;
}

#lesson_panel .lesson_icon
{
	/*width: 1000px;*/
	min-width: 96%;
	height: 120px;
	background-color: #DFEFFC;
	overflow-x: auto;
}

#lesson_panel .lesson_icon:hover
{
	background-color: #FCE877 ! important;
	cursor: hand;
	cursor: pointer;
}

#lesson_panel .lesson_icon .lesson_name
{
	text-align: left !important;
	margin: 0px 0px 10px 0px;
}

#lesson_panel .lesson_icon .lesson_name .label
{
	color: #2E6E9E;
	margin: 0px 5px 0px 0px;
}

#lesson_panel .lesson_icon .image_list
{
	/*width: 2000px;*/
	
}

#lesson_panel .lesson_icon .image_list .image
{
	float: left;
	margin: 0px 5px;
	border: 1px solid #4297D7;
}

#lesson_panel .lesson_icon .image_list .image center
{
	font-size: 12px;
	margin: -5px 0px 0px 5px;
}

#session_panel .session_icon
{
	background-color: #DFEFFC;
}

#session_panel .session_icon:hover
{
	background-color: #FCE877 ! important;
	cursor: hand;
	cursor: pointer;
}

#session_panel .session_icon .thumbnail_container
{
	background-color: #FFFFFF;
	float: none ! important;
	margin: 0 5px;
	-moz-box-shadow: 0px 0px 0px #000;
}

#session_dialog
{
	min-width: 500px;
	min-height: 300px;
	padding: 10px 20px 20px 10px;
	color: #2E6E9E;
}

#session_dialog .pupil_label
{
	text-align: left;
	float: left;

}

#session_dialog .tutor_label
{
	text-align: left;
	float: left;
}

#session_dialog .pupil_icon
{
	float: none;
	cursor: hand; cursor: pointer;
	background-color: #DFEFFC;
}

#session_dialog .pupil_icon #pupil_name
{
	color: #000000;
	font-size: 12px;
	float: left;
	padding: 0px 0px 0px 5px;
}

#session_dialog .pupil_icon .icon
{
	margin: 5px;
	min-width: 100px;
	clear: both;
}

#session_dialog .tutor_icon
{
	float: none;
	cursor: hand; cursor: pointer;
	background-color: #DFEFFC;
	
}

#session_dialog .tutor_icon #tutor_name
{
	color: #000000;
	font-size: 12px;
	float: left;
	padding: 0px 0px 0px 5px;
}


#session_dialog .tutor_icon .icon
{
	margin: 5px;
	min-width: 100px;
	clear: both;
}


#session_dialog .lesson_icon
{
	cursor: hand; cursor: pointer;
	background-color: #DFEFFC;
	min-height: 100px;
	min-width: 500px;
}

#session_dialog .lesson_icon .mini_icon
{
	-moz-box-shadow: 0px 0px 0px #000000 ! important;
}

#session_dialog .lesson_icon .mini_icon:hover
{
	background-color: #FFFFFF ! important;
}

#session_dialog .lesson_icon .mini_icon .name
{
	text-align: center;
}

#session_dialog .lesson_icon .lesson_name
{
	padding: 0px 0px 5px 0px;
}

#session_dialog .lesson_icon #lesson_name
{
	margin-left: 5px;
	color: #000000;
}

.dialog_icon .image
{
	min-height: 100px;
	min-width: 100px;
	height: auto;
	width: auto;
	color: #000000;
}

.dialog_icon .image img
{
	width: 100px;
	height: auto;
}

.dialog .column
{
	float: left;
	margin: 10px 20px 10px 10px;
}

.dialog #id
{
  width: 185px;
}

.dialog #require_note
{
	display: inline-block;
	float: right;
	color: red;
	font-size: 10px;
	margin-bottom: 10px;
}

.dialog #upload
{
	float: left;
	margin: 10px;
}

.dialog div#attributes
{
	float: left;
	margin: 30px 10px 10px 10px;
}

.dialog .question
{
	display: inline-block;
	padding: 5px;
	min-width: 200px;
	width: 300px;
	margin: 0px 0px -3px 0px;
	color: #2E6E9E;
}

.dialog .question:hover
{
	background-color: #FCE877 ! important;
	color: #2E6E9E ! important;
	cursor: hand;
	cursor: pointer;
}

.dialog .selected_question
{
	background-color: #2E6E9E ! important;
	color: #FCE877;
}

.dialog .question.bg1
{
	background-color: #D0E5F5;
}

.dialog .question.bg2
{
	background-color: #DFEFFC;
}

.dialog .question #text
{
	float: right;
	margin: 0px 30px 0px 0px;
}

.dialog .question #order_number
{
	float: left;
	width: 40px;
}

.dialog #new_question_container
{
	padding: 5px;
	color: #E17009;
	border-bottom: 1px solid #C5DBEC;
	text-align: right;
}

.dialog #new_question:hover, .dialog #load_question:hover
{
	cursor: hand;
	cursor: pointer;
}

.dialog #new_question_container .text
{
	margin-right: 68px;
	color: #2E6E9E;
}

.dialog #new_question_container #new_question
{
	float: right;
}

.dialog #new_question_container #load_question
{
	float: left;
}

.dialog #question_list_container
{
	min-height: 100px;
	min-width: 200px;
	border: 1px solid #4297D7;
	-moz-border-radius: 10px 10px 0px 0px;
}

.dialog #question_list
{
	max-height: 300px;
	width: 300px;
	overflow-y: auto;
	overflow-x: hidden;
}

.validation_error_box
{
	position: absolute;
	font-size: 10px;
	line-height: 15px;
	height: 12px;
	/*width: 330px;*/
	width: auto;
	float:left;
	margin: 0 0 0 0px;
	padding: 5px;
	overflow: hidden;
	color: #9E2E2E ! important;
	background: #FCDFDF ! important;
	border: 1px solid #ECC5C5 ! important;
	-moz-border-radius: 0 5px 5px 0;
}

.pullout_image_list
{
	position: absolute;
	line-height: 15px;
	height: auto;
	max-height: 200px;
	max-width: 300px;
	float:left;
	margin: 0 0 0 0px;
	padding: 5px;
	overflow-y: auto;

	color: #2E6E9E ! important;
	background: #DFEFFC ! important;
	border: 1px solid #C5DBEC ! important;
	-moz-border-radius: 5px 5px 5px 5px;
	-moz-box-shadow: 10px 10px 5px #888888;
}

.pullout_image_list .image_list
{
	width: 110%;
}

.pullout_image_list .header
{
	border-bottom: 1px solid #C5DBEC;
	display: inline-block;
	width: 100%;
}

.pullout_image_list .header .label
{
	display: inline-block;
	float: left;
}

.pullout_image_list .header .close
{
	display: inline-block;
	float: right;
	text-decoration: underline;
}

.pullout_image_list .header a
{
	float: right;
}

.pullout_image_list .header a:hover
{
	background-color: #2E6E9E;
	color: #DFEFFC ! important;
	cursor: hand;
	cursor: pointer;

}

.pullout_image_list .content
{
	display: inline-block;
	overflow: hidden;
}

.ui-accordion-content
{
	padding: 5px !important;
}

.hidden
{
	display: none !important;
}

.selected_item
{
	background-color: #FCE877 ! important;
}