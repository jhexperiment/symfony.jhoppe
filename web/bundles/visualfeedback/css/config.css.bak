table
{
	border-collapse: separate ! important;
}
center
{
	margin: 0;
	padding: 0;
}

#content
{
	display: inline-block;
}

#tab_container
{
	width: 700px;
	margin-top: 50px;
	padding: 0px 10px;
	display: inline-block;
	position: relative;
}

#tab_container .tab:hover
{
	background-color: #FCE877 ! important;
	color: #4D81A8 ! important;
}

#tab_container .tab {
  cursor: pointer;
	display: inline-block;
	margin-bottom: -6px;
	float: left;
	width: 115px;
	line-height: 40px;
	font-size: large;
	text-align: center;
	vertical-align: bottom;
	background-color: #70C2FF;
	color: white;
	font-weight: bold;
	padding: 0px 5px;
	border-width: 0px 1px ! important;
	border-style: solid;
	border-color: white;
}

#tab_container .first_tab
{
	border-width: 0px 1px 0px 0px ! important;
	border-style: solid;
	border-color: white;
	-moz-border-radius: 10px 0 0 0;
	border-radius: 10px 0 0 0;
}

#tab_container .last_tab
{
	border-width: 0px 0px 0px 1px ! important;
	border-style: solid;
	border-color: white;
	-moz-border-radius: 0 10px 0 0;
	border-radius: 0 10px 0 0;
}

#tab_container .selected_tab
{
	background-color: #5C9CCC ! important
}

#menu
{
	background-color: #5289B3;
	color: white;
	position: absolute;
	left: 0; top: 0;
	padding: 0px 0px 3px 0px;
	overflow: hidden;
	-moz-box-shadow: 5px 5px 5px #888;
	-webkit-box-shadow: 5px 5px 5px #888;
	box-shadow: 5px 5px 5px #888;
}

#menu .menu_item
{
	text-align: center;
	font-size: medium;
	line-height: 30px;
	margin: 5px;
}

#menu .menu_item:hover
{
	background-color: #FCE877;
}

#panel_container
{
	display: inline-block;
	position: relative;
	padding: 20px;
	min-width: 900px;
	border: 1px solid #4297D7;
	-moz-border-radius: 10px;
	border-radius: 10px;
	
}

#panel_container .panel
{
	position: absolute;
	display: inline-block;
	position: relative;
	left:0;
	width: 100%;
}

.data_point
{
	clear: both;
	display: inline-block;
}



#lesson_dialog .data_point
{
	display: block;
	color: #2E6E9E;
}

.data_point .label, .data_point .input, .data_point .text
{
	float: left;
	
}

.data_point .label
{
	line-height: 24px;
	width: 140px;
	text-align: left;
}

#lesson_dialog .data_point .label
{
	width: auto ! important;
	margin-right: 5px;
	color: #2E6E9E;
}

.panel .control_container
{
	display:inline-block;
	margin-left:auto;
	margin-right:auto;
	max-width:600px;
	text-align:left;
}

#add_tutor, #add_pupil, #add_image, #add_session, #session_save_all
{
	float:right;
	margin-left: 10px;	
}

#add_image
{
	
}

.panel_data
{
	margin-top: 20px;
}

.save_input
{
	float: right;
}

#image_form div
{
	float: left;
}

#image_form .label
{
	width: 100px;
	line-height: 24px;
}

#image_form .save_input
{
	float:right;
}

#image_form_container
{
	display: inline-block;
}

#image_upload_return
{
	float:left;
	font-size:14px;
	line-height:30px;
}

.thumbnail_container
{
	float:left;
	padding: 5px;
	margin: 0px 20px 20px 0px;
	border: 1px solid #4297D7;
	-moz-border-radius: 10px;
	border-radius: 10px;
	-moz-box-shadow: 10px 10px 5px #888888;
	-webkit-box-shadow: 10px 10px 5px #888888;
	box-shadow: 10px 10px 5px #888888;
}

.thumbnail_container img
{	
	height: 100px;
}

.thumbnail_container .image
{
	background-color: white;
	padding: 5px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	height: 100px;
	
	width: auto;
	text-align: center;
	font-size: 40px;
}

#pupil_dialog .thumbnail_container, #tutor_dialog .thumbnail_container
{
  -moz-box-shadow: 0 0 0 #FFFFFF;
  
}

#pupil_dialog .thumbnail_container:hover, #tutor_dialog .thumbnail_container:hover
{
  background-color: white ! important;
}

#pupil_dialog .icon_list
{
  
}

#pupil_dialog .pupil_icon .image
{
  min-height: 100px;
  line-height: auto;
	height: auto;
	width: 100px;
}

#pupil_dialog .pupil_icon .image img
{
	width: 100px;
	height: auto;
}

#accordion
{
	height: auto;
  width: auto;
  clear: right;
  float: left;
	margin: 10px;
}

#pupil_dialog .image_icon
{
    margin: 0 0 10px 10px ! important;
}

#pupil_dialog .image_icon .image
{
  height: auto;
}

#pupil_dialog .image_icon img, #pupil_dialog .image_icon .image
{
  height: 40px;
}

#pupil_panel .pupil_icon:hover
{
	background-color: #FCE877 ! important;
	cursor: hand; cursor: pointer;
}


#tutor_panel .tutor_icon:hover
{
	background-color: #FCE877 ! important;
	cursor: hand; cursor: pointer;
}




#session_table .action_col
{
	border: none ! important;
	background-color: white ! important;
}

select
{
	height: 0;
}
